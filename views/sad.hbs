  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous">
  <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.0/font/bootstrap-icons.css">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.bundle.min.js" integrity="sha384-b5kHyXgcpbZJO/tY9Ul7kGkf1S0CWuKcCD38l8YkeH8z8QjE0GmW1gYU5S9FOnJ0" crossorigin="anonymous"></script>
  <link rel="stylesheet" href="/stylesheets/main.css">
  <script>
      $.get( "/login", function(user) { user = user;});
  </script>

  <title>Scent - Sad</title>

</head>

<body>
  <header class="sticky-top">
    <div class="navbar navbar-light bg-light shadow-sm">
      <div class="container d-flex justify-content-between">
        <a href="#" class="navbar-brand d-flex align-items-center">
      <img src="assets/logo.png" width="90em" height="auto" alt="">
      </a>
      <a href="/logout" class="navbar-brand d-flex align-items-center">
        <button type="button" class="btn btn-outline-secondary">
          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-box-arrow-right" viewBox="0 0 16 16">
            <path fill-rule="evenodd" d="M10 12.5a.5.5 0 0 1-.5.5h-8a.5.5 0 0 1-.5-.5v-9a.5.5 0 0 1 .5-.5h8a.5.5 0 0 1 .5.5v2a.5.5 0 0 0 1 0v-2A1.5 1.5 0 0 0 9.5 2h-8A1.5 1.5 0 0 0 0 3.5v9A1.5 1.5 0 0 0 1.5 14h8a1.5 1.5 0 0 0 1.5-1.5v-2a.5.5 0 0 0-1 0v2z"/>
            <path fill-rule="evenodd" d="M15.854 8.354a.5.5 0 0 0 0-.708l-3-3a.5.5 0 0 0-.708.708L14.293 7.5H5.5a.5.5 0 0 0 0 1h8.793l-2.147 2.146a.5.5 0 0 0 .708.708l3-3z"/>
        </svg> log out
        </button>
        </a>
        </div>
    </div>
  </header>

  <main class="pb-5 mb-5">
    <section class="text-center p-5">
      <div class="container">
        <h1>Sad</h1>
        <p>
          <a href="/upload" class="btn col-4 btn-block">Add More</a>
      </p>
      </div>
    </section>
    {{!-- </div> --}}

    <section class="container pb-5 mb-5">
          <section class="row">
              {{#each person}}
              <div class="col-md-3">
                <div class="card mb-4 box-shadow">
                  <img src = "/images/{{imageName}}" class="card-img-top" alt="emotions" width="60%" height="auto">
                  <div class="card-body">
                  <small class="text-muted">{{name}} </small>
                    <p class="card-text">
                      {{emotion.[0]}}&nbsp{{level.[0]}}</br>{{emotion.[1]}}&nbsp{{level.[1]}}</br>{{emotion.[2]}}&nbsp{{level.[2]}}</br>{{emotion.[3]}}&nbsp{{level.[3]}}</br>{{emotion.[4]}}&nbsp{{level.[4]}}
                    </p>
                </div>
              </div>
          </div>
        {{/each}}
    </section>
  </main>

  <footer class="fixed-bottom bg-light p-2">
      <ul class="nav justify-content-center nav-justified">
       
